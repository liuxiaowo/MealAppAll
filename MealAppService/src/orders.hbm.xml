<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bean.orders" table="orders">
		<id name="id">
			<generator class="identity" />

		</id>
		<property name="userid" />
		<property name="username" />
		<property name="price" />
		<property name="createtime" />
		<property name="status" />
		<property name="deliveryway"></property>
		<property name="deliverytime"></property>
		<property name="payway"></property>
		<property name="addressid"></property>
		<property name="node"></property>	
		<property name="orderid"></property>
	</class>

</hibernate-mapping>